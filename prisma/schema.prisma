generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Fatura {
  id                    Int      @id @default(autoincrement())
  numCliente            String
  numInstalacao         String
  mesReferencia         String
  energiaEletricaKwh    Float
  energiaEletricaValor  Float
  energiaSceeeKwh       Float
  energiaSceeeValor     Float
  energiaCompensadaGdi  Float
  valorTotal            Float
  valorEconomiaGd       Float
  contribuicaoIlum      Float
  createdAt             DateTime @default(now())
}
